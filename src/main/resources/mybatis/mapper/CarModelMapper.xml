<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.exam.dao.mapper.CarModelMapper">


    <select id="selectCarModelByCarModelId" resultType="com.exam.module.entity.CarModel">
        select * from car_model where id = #{carModelId}
    </select>
    <select id="selectCarModels" resultType="com.exam.module.entity.CarModel">
        select * from car_model
    </select>
</mapper>
